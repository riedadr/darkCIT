{
	"name": "Dark CIT",
	"colors": {
		//elements from left to right
		"activityBar.background": "#1d212f",
		"activityBar.border": "#334155",
		"activityBar.activeBackground": "#334155",
		"activityBar.activeBorder": "#ff5000",

		"sideBar.background": "#1d212f",
		"sideBarSectionHeader.background": "#1d212f",
		"sideBarSectionHeader.border": "#334155",
		"list.hoverBackground": "#334155",
		"list.activeSelectionBackground": "#ff5000",
		"list.inactiveSelectionBackground": "#334155",

		"editorGutter.background": "#1d212f",
		"editor.background": "#1d212f",

		//elements from top to bottom
		"titleBar.activeBackground": "#1d212f",
		"editorGroupHeader.tabsBackground": "#1d212f",
		"tab.inactiveBackground": "#1d212f",
		"tab.activeBackground": "#1d212f",
		"tab.activeBorder": "#ff5000",
		"tab.hoverBackground": "#1d212f",
		"tab.hoverBorder": "#ff5000",
		"statusBar.background": "#1d212f",
		"statusBar.border": "#334155",

		//terminal
		"terminal.background": "#1d212f",
		"terminal.ansiBlack": "#334155",
		"terminal.ansiBrightBlack": "#64748b",
		"terminal.ansiBlue": "#3b82f6",
		"terminal.ansiBrightBlue": "#38bdf8",
		"terminal.ansiBrightCyan": "#67e8f9",
		"terminal.ansiBrightGreen": "#4ade80",
		"terminal.ansiBrightMagenta": "#f472b6",
		"terminal.ansiBrightRed": "#fb7185",
		"terminal.ansiBrightWhite": "#ffffff",
		"terminal.ansiBrightYellow": "#facc15",
		"terminal.ansiCyan": "#06b6d4",
		"terminal.ansiGreen": "#10b981",
		"terminal.ansiMagenta": "#ec4899",
		"terminal.ansiRed": "#ef4444",
		"terminal.ansiWhite": "#ffffff",
		"terminal.ansiYellow": "#f59e0b",
		"terminal.findMatchBackground": "#10b981",
		"terminal.selectionBackground": "#ff5000",
		"terminal.tab.activeBorder": "#ff5000",

		"terminalCursor.background": "#000",
		"terminalCursor.foreground": "#fff",

		"button.background": "#ff5000",
		"extensionButton.prominentBackground": "#10b981",
		"input.background": "#1d212f",
		"input.border": "#334155",

		//badge (shows number of changes at git, etc)
		"activityBarBadge.background": "#ff5000",
		"activityBarBadge.foreground": "#eceff4"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#475569"
			}
		},
		{
			"name": "Variables",
			"scope": ["variable", "string constant.other.placeholder"],
			"settings": {
				"foreground": "#EEFFFF"
			}
		},
		{
			"name": "Colors",
			"scope": ["constant.other.color"],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"foreground": "#db2777"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#d946ef"
			}
		},
		{
			//import, export, return
			"name": "Keyword Control",
			"scope": ["keyword.control"],
			"settings": {
				"foreground": "#8b5cf6"
			}
		},
		{
			//text inside tags
			"name": "Operator, Misc",
			"scope": [
				//"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			//common html tags
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#f97316",
				"fontStyle": "bold"
			}
		},
		{
			//functions
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#0ea5e9",
				"fontStyle": ""
			}
		},
		{
			//console, localStorage, imports
			"name": "Block Level Variables",
			"scope": ["meta.block variable.other"],
			"settings": {
				"foreground": "#ec4899"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": ["support.other.variable", "string.other.link"],
			"settings": {
				"foreground": "#ec4899"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#eab308"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#10b981"
			}
		},
		{
			//components (React, JSX, TSX)
			"name": "Class, Support",
			"scope": [
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#FFCB6B"
			}
		},
		{
			//typescript types
			"name": "Types",
			"scope": ["entity.name", "support.type"],
			"settings": {
				"foreground": "#3b82f6",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Entity Types",
			"scope": ["support.type"],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},

		{
			"name": "Sub-methods",
			"scope": [
				"entity.name.module.js",
				"variable.import.parameter.js",
				"variable.other.class.js"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Language methods",
			"scope": ["variable.language"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FF5370"
			}
		},
		{
			"name": "entity.name.method.js",
			"scope": ["entity.name.method.js"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "meta.method.js",
			"scope": [
				"meta.class-method.js entity.name.function.js",
				"variable.function.constructor"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			//html-attributes like id, className, etc
			"name": "Attributes",
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#f59e0b",
				"fontStyle": "italic"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html",
				"text.html.basic entity.other.attribute-name"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#f59e0b"
			}
		},
		//CSS
		{
			"name": "CSS Classes",
			"scope": ["entity.other.attribute-name.class"],
			"settings": {
				"foreground": "#d946ef"
			}
		},
		{
			"name": "CSS ID's",
			"scope": [
				"source.sass keyword.control",
				"entity.other.attribute-name.id"
			],
			"settings": {
				"foreground": "#3b82f6"
			}
		},
		{
			"name": "CSS Class and Support",
			//CSS Propertiy
			"scope": [
				"source.css support.type.property-name",
				"source.sass support.type.property-name",
				"source.scss support.type.property-name",
				"source.less support.type.property-name",
				"source.stylus support.type.property-name",
				"source.postcss support.type.property-name"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
		{
			"name": "Inserted",
			"scope": ["markup.inserted"],
			"settings": {
				"foreground": "#4ade80"
			}
		},
		{
			"name": "Deleted",
			"scope": ["markup.deleted"],
			"settings": {
				"foreground": "#fb7185"
			}
		},
		{
			"name": "Changed",
			"scope": ["markup.changed"],
			"settings": {
				"foreground": "#c084fc"
			}
		},
		{
			"name": "Regular Expressions",
			"scope": ["string.regexp"],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "Escape Characters",
			"scope": ["constant.character.escape"],
			"settings": {
				"foreground": "#89DDFF"
			}
		},
		{
			"name": "URL",
			"scope": ["*url*", "*link*", "*uri*"],
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Decorators",
			"scope": [
				"tag.decorator.js entity.name.tag.js",
				"tag.decorator.js punctuation.definition.tag.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "ES7 Bind Operator",
			"scope": [
				"source.js constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#494243"
			}
		},
